{
  "name": "password-validator",
  "version": "4.0.0",
  "description": "Validates password according to flexible and intuitive specifications",
  "main": "src/index.js",
  "scripts": {
    "docs": "node_modules/.bin/typedoc ./src/*.ts --theme ./node_modules/docdash --readme README.md --out ./docs --module commonjs",
    "lint": "tslint ./src",
    "test:unit": "./node_modules/.bin/mocha -r ts-node/register tests/**/*.ts --timeout 15000",
    "test": "npm run lint && npm run test:unit",
    "build": "./node_modules/.bin/tsc",
    "start": "node index.js"
  },
  "keywords": [
    "password",
    "validator",
    "rules",
    "schema",
    "validation",
    "check",
    "match"
  ],
